<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <system.serviceModel>
    <services>
      <service name="Exercise.Server">
        <host>
          <baseAddresses>
            <add baseAddress = "net.tcp://localhost:8732/Service"/>
            <add baseAddress="http://localhost:5050/Service"/>
          </baseAddresses>
        </host>
       
        <endpoint address="http://localhost:5050/Service" binding="wsDualHttpBinding" contract="Exercise.IServer"/>
        <!--<endpoint address="mex" binding="mexTcpBinding" contract="Exercise.IServer" />-->
        <!--<endpoint address="tcp" binding="netTcpBinding" contract="Exercise.IServer" />-->

       
      </service>
    </services>
   
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
          <!--<serviceMetadata/>-->
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>